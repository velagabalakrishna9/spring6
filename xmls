	version="2.4">
	<display-name>coi</display-name>
	<distributable />
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>
    		/WEB-INF/coi-security.xml 
    		/WEB-INF/coi-servlet.xml
    	   /WEB-INF/spring-datasource.xml
		</param-value>
	</context-param>
